CREATE TABLE
    `HCAHPS-copy` (
        `Facility ID` INTEGER,
        `Facility Name` TEXT,
        `Address` BLOB,
        `City/Town` TEXT,
        `State` TEXT,
        `ZIP Code` INTEGER,
        `County/Parish` TEXT,
        `Telephone Number` BLOB,
        `HCAHPS Measure ID` BLOB,
        `HCAHPS Question` BLOB,
        `HCAHPS Answer Description` BLOB,
        `HCAHPS Answer Percent` REAL,
        `Number of Completed Surveys` INTEGER,
        `Survey Response Rate Percent` REAL,
        `Start Date` date,
        `End Date` date
    );

INSERT INTO
    `HCAHPS-copy`
SELECT
    *
FROM
    `HCAHPS`;
