CREATE TABLE
    `hospital_beds-copy` (
        `Provider CCN` INTEGER,
        `Hospital Name` BLOB,
        `Fiscal Year Begin Date` date,
        `Fiscal Year End Date` date,
        `number_of_beds` INTEGER
    );

INSERT INTO
    `hospital_beds-copy`
SELECT
    *
FROM
    `hospital_beds`;
